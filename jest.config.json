{
  "collectCoverage": true,
  "moduleFileExtensions": [
    "js",
    "mjs"
  ],
  "transform": {
    "^.+\\.js$": "babel-jest",
    "^.+\\.mjs$": "babel-jest"
  },
  "testRegex": "((\\.|/*.)(test))\\.js?$",
  "modulePathIgnorePatterns": [
    "<rootDir>/src/config",
    "<rootDir>/tmp",
    "<rootDir>/src/models/pg"
  ]
}
